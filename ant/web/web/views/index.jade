//-	首页模版
doctype html
html
	head
		title ANT - 蚁逅 | 一群蚂蚁的邂逅
		//-	信息
		meta(name="keywords", content="ANT,蚁逅,在线网络安全工具,网络安全云平台")
		meta(name="description", content="蚁逅(ANT)是一款专注网络安全的在线学习交流与分享平台")
		//-	加载页面
		link(href="/ant/css/load.css", rel="stylesheet")
		//-	图标
		link(href="/ant/img/favorite.png", rel="shortcut icon")
		//-	加载CSS
		link(href="/w2ui/w2ui-1.4.2.min.css", rel="stylesheet")
		link(href="/bootstrap/dist/css/bootstrap.min.css", rel="stylesheet")
		link(href="/font-awesome-4.3.0/css/font-awesome.min.css", rel="stylesheet")
		link(href="/toastr/toastr.css", rel="stylesheet")
		link(href="/ant/css/index.css", rel="stylesheet")
		//-	加载JS
		script(src="/jquery/dist/jquery.min.js")
		script(src="/toastr/toastr.min.js")
		script(src="/w2ui/w2ui-1.4.2.min.js")
		//-	ACE编辑器
		script(src="/ace/ace.js")
		script(src="/ace/theme-clouds.js")
		script(src="/ace/theme-tomorrow.js")
		script(src="/ace/worker-javascript.js")
		script(src="/ace/mode-javascript.js")
		script(src="/ace/mode-markdown.js")
		script(src="/ace/mode-python.js")
		//-	七牛云储存
		script(src="/plupload-2.1.3/js/plupload.full.min.js")
		script(src="/qiniu-js-sdk/src/qiniu.min.js")
		//-	自定义JS
		script(src="/ant/js/socket.io.min.js")
		script(src="/ant/js/index.min.js")
	body
		div#loading
			article.preloader
				section.satelite
				section.earth.day
		div#ant.hidden-xs
		div.hidden-md.hidden-lg.hidden-sm(align="center")
			a(href="http://weibo.com/antoor")
				img(src="/ant/img/qr.png", style="width: 100%;height: 100%;")